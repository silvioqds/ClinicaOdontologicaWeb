@model IEnumerable<ClinicaOdontologica.Desfalcados.Models.Users>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<p>
    <button class="btn btn-light">@Html.ActionLink("Adicionar Novo", "Create")</button>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>        
        <th>
            @Html.DisplayName("Ativo")
        </th>   
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Username)
        </td>       
        <td>
            @Html.DisplayFor(modelItem => item.active)
        </td>
       
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.UsuarioID }) |            
            @Html.ActionLink("Excluir", "Delete", new { id=item.UsuarioID })
        </td>
    </tr>
}

</table>
